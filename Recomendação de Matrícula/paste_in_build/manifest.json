{
	"name":"Recomendação de Matrícula",
	"version":"1.0",
	"description":"Recomenda disciplinas ao aluno regularmente matrículado na Universidade de Brasília - GAMA (FGA)",
	"manifest_version": 3,
	"icons": {
	"128": "./icons/logo128.png"
	},
	
	"permissions": [
		"tabs",
		"storage"
	],

	"background":{
		"scripts": ["./background/background.js"]
	},

	"browser_action":{
		"default_popup":"index.html"
	},

	"content_scripts":[
		{
			"matches": ["https://matriculaweb.unb.br/graduacao/sec/qr.aspx"],
			"js": ["./content/quadroresumo.js"]
		}
	],

	"content_security_policy": "script-src 'self' 'sha256-Sgnctzgh2eeetJOaBSHeUK/gFtY+Bq0P3uE+iabHQmY='; object-src 'self'"

}
